<%= form_with model: @lodging do |f| %>
  <%= f.label :home_type %>
  <%= f.text_field :home_type %>
  <%= f.label :total_occupancy %>
  <%= f.number_field :total_occupancy %>
  <%= f.label :summary %>
  <%= f.text_field :summary %>
  <%= f.check_box :park_dog %>
  <%= f.label :park_dog %>
  <%= f.check_box :park_cat %>
  <%= f.label :park_cat %>
  <%= f.check_box :bath_service %>
  <%= f.label :bath_service %>
  <%= f.check_box :walk_service %>
  <%= f.label :walk_service %>
  <%= f.check_box :special_care %>
  <%= f.label :special_care %>
  <%= f.check_box :place_with_animals %>
  <%= f.label :place_with_animals %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :animals_taken_care_of %>
  <%= f.text_field :animals_taken_care_of %>
  <%= f.submit %>
<% end %>

<%= link_to "Volver al listado", lodgings_path %>
